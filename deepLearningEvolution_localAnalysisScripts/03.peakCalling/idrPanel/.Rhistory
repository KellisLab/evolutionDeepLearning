library(ggpubr)
data <- read.csv("testIdr.txt", header=FALSE, sep="\t")
head(data)
colnames(data) <- c("Chrom", "Start1", "End1", "Name", "Score", "Strand", "Signal1", "Signal2", "Start2", "End2", "OverlapLength", "StartPooled", "EndPooled", "IdrScore")
head(data)
data$Rank1 <- rank(-data$Signal1)
data$Rank2 <- rank(-data$Signal2)
head(data)
ggplot(data, aes(x = Rank1, y = Rank2, color = IdrScore)) +
geom_point() +
scale_color_gradient(low = "blue", high = "red") +
labs(
title = "Rank1 vs Rank2 with IDR Score",
x = "Rank1 (based on Signal1)",
y = "Rank2 (based on Signal2)",
color = "IDR Score"
) +
theme_minimal()
ggscatter(data, "Signal1", "Signal2", color="IdrScore")
